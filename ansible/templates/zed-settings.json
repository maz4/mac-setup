{
  "vim_mode": true,
  "theme": "VScode Dark Plus",
  "ui_font_size": 16,
  "buffer_font_size": 14,
  "buffer_font_family": "Monaco",
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "features": {
    "copilot": false
  },
  "terminal": {
    "font_family": "Inconsolata for Powerline",
    "font_size": 16
  },
  "autosave": "on_window_change"
}
